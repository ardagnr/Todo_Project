  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "types": ["node", "express"]
  },
  "include": [
    "src/**/*", 
    "src/types/express.d.ts" 
  ]
  ,
  "files":["src/types/express.d.ts"] 
}
